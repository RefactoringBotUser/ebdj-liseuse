<BorderPane fx:id="panneauStats" maxHeight="-Infinity" maxWidth="1600" minHeight="-Infinity" minWidth="-Infinity" prefHeight="360.0" prefWidth="1024.0" style="-fx-background-color: #FFFFFF; -fx-border-radius: 12px;" BorderPane.alignment="CENTER" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="fr.qp1c.ebdj.controller.stats.StatistiquesController">
   <top>
      <BorderPane prefHeight="60.0" prefWidth="60.0" BorderPane.alignment="CENTER">
