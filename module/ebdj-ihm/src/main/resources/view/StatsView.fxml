                           	<TableColumn fx:id="tabSFColQuantiteJouee" editable="false" prefWidth="183.0" sortable="false" text="Quantité jouée" />
                            </columns>
                        </TableView>
                           </children>
                        </VBox>
                     </top>
                </BorderPane>
              </content>
            </TitledPane>
        </panes>
      </Accordion>
   </center>
