CREATE TABLE "THEME_QALS" ( `id` INTEGER, `categorie` TEXT NOT NULL, `categorieRef` INTEGER NOT NULL, `groupeCategorieRef` INTEGER NOT NULL, `theme` TEXT NOT NULL, `difficulte` INTEGER NOT NULL, `reference` INTEGER NOT NULL UNIQUE, `club` TEXT NOT NULL, `dateReception` TEXT NOT NULL, `version` INTEGER NOT NULL, `active` INTEGER NOT NULL, PRIMARY KEY(`id`) );

CREATE TABLE "THEME_QALS_ANOMALIE" ( `id` INTEGER NOT NULL, `reference` INTEGER NOT NULL, `version` INTEGER NOT NULL, `date_anomalie` TEXT NOT NULL, `type_anomalie` INTEGER NOT NULL, `cause` TEXT NOT NULL, `lecteur` TEXT NOT NULL, PRIMARY KEY(`id`) );

CREATE TABLE "THEME_QALS_LECTURE" ( `id` INTEGER NOT NULL, `date_lecture` DATETIME NOT NULL, `lecteur` TEXT, `reference` INTEGER NOT NULL, PRIMARY KEY(`id`) );

CREATE TABLE "THEME_QALS_PRESENTE" ( `id` INTEGER NOT NULL, `date_presentation` DATETIME NOT NULL, `lecteur` TEXT, `reference` INTEGER NOT NULL, PRIMARY KEY(`id`) );

CREATE TABLE "QUESTION_QALS" ( `id` INTEGER, `seq` INTEGER NOT NULL, `question` TEXT NOT NULL, `reponse` TEXT NOT NULL, `reference` INTEGER NOT NULL, PRIMARY KEY(`id`) );